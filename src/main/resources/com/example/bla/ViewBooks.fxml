<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bla.ViewBooks">
   <children>
      <Label layoutX="91.0" layoutY="30.0" prefHeight="30.0" prefWidth="206.0" text="Available books:" textFill="CHOCOLATE">
         <font>
            <Font name="Arial Rounded MT Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="exit" layoutX="258.0" layoutY="348.0" mnemonicParsing="false" style="-fx-background-color: #D2691E;" text="Go Back">
         <font>
            <Font name="Arial Rounded MT Bold" size="15.0" />
         </font>
      </Button>
      <TableView fx:id="books" layoutX="140.0" layoutY="67.0" prefHeight="250.0" prefWidth="336.0">
        <columns>
          <TableColumn fx:id="bookName_col" prefWidth="101.0" text="Book Name" />
          <TableColumn fx:id="bookPrice_col" prefWidth="118.0" text="Book Price" />
            <TableColumn fx:id="username_col" prefWidth="116.0" text="Author" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
